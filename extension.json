{
  "id": "builtin.language-basics-dotenv",
  "name": "Language Basics Dotenv",
  "description": "Provides syntax highlighting and bracket matching in dotenv files.",
  "languages": [
    {
      "id": "dotenv",
      "extensions": [".env"],
      "fileNames": [
        "sampledotenv",
        ".env.local",
        ".env.example",
        ".env.development.example",
        ".env.production.example",
        ".env.development",
        ".env.production",
        ".env.dev",
        ".env.sample",
        ".env.dist",
        ".env.test"
      ],
      "configuration": "./languageConfiguration.json",
      "tokenize": "src/tokenizeDotenv.js"
    }
  ]
}
